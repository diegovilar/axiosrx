{
  "private": true,
  "name": "@diegovilar/axiosrx",
  "version": "0.1.0-rc.1",
  "description": "Axios with Rx.Observables",
  "license": "MIT",
  "author": "Diego Vilar <diegovilar@gmail.com> (https://github.com/diegovilar)",
  "repository": "github:diegovilar/axiosrx",
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.0.0",
    "yarn": ">=1.0.0"
  },
  "main": "bundles/lib.withdeps.umd.js",
  "module": "esm5/index.js",
  "es2015": "esm2015/index.js",
  "typings": "index.d.ts",
  "scripts": {
    "prepublishOnly": "yarn run build && yarn run test",
    "clean": "rimraf build __test__ coverage .nyc_output .awcache node_modules/.cache/hard-source",
    "prebuild": "rimraf build && tsc -p tsconfig.typings.json",
    "build": "concurrently -n \"esm5,esm2015,bundles\" -c \"green,cyan,magenta\" \"yarn run build-esm5\" \"yarn run build-esm2015\" \"yarn run build-bundles\"",
    "postbuild": "ts-node -P scripts/tsconfig.json scripts/postbuild.ts",
    "build-esm5": "rimraf build/esm5 && tsc -p tsconfig.esm5.json",
    "build-esm2015": "rimraf build/esm2015 && tsc -p tsconfig.esm2015.json",
    "build-bundles": "rimraf build/bundles && webpack --config webpack.config.js",
    "build-test": "rimraf __test__ && tsc -p test/tsconfig.json",
    "test": "yarn run build-test && mocha --opts mocha.opts",
    "test+coverage": "rimraf coverage && nyc --nycrc-path nyc.json yarn run test",
    "tslint": "tslint -p tsconfig.esm5.json -c src/tslint.json --fix"
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/mocha": "^2.2.48",
    "@types/node": "~8.9.4",
    "@types/sinon": "^4.3.0",
    "awesome-typescript-loader": "^4.0.1",
    "chai": "^4.1.2",
    "clone": "^2.1.1",
    "codelyzer": "^4.2.1",
    "concurrently": "^3.5.1",
    "hard-source-webpack-plugin": "^0.5.18",
    "mocha": "^5.0.4",
    "mocha-typescript": "^1.1.12",
    "nyc": "~11.5.0",
    "rimraf": "^2.6.2",
    "rxjs": "^5.5.6",
    "sinon": "^4.3.0",
    "source-map-loader": "^0.2.3",
    "source-map-support": "^0.5.4",
    "ts-loader": "^3.5.0",
    "ts-node": "^5.0.1",
    "tsconfig-paths-webpack-plugin": "^2.0.0",
    "tslint": "^5.9.1",
    "typescript": "^2.8.0-rc",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "webpack": "^3.11.0"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "tslib": "^1.9.0"
  },
  "peerDependencies": {
    "rxjs": "^5.5.6"
  }
}
