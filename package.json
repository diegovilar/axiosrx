{
  "name": "@diegovilar/axiosrx",
  "version": "0.0.0-alpha.4",
  "description": "Axios with Rx.Observables",
  "license": "MIT",
  "author": "Diego Vilar <diegovilar@gmail.com> (https://github.com/diegovilar)",
  "repository": "github:user/repo",
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.0.0",
    "yarn": ">=1.0.0"
  },
  "main": "build/es5/index.js",
  "module": "build/es6/index.js",
  "jsnext:main": "build/es6/index.js",
  "types": "build/es5/index.d.ts",
  "typings": "build/es5/index.d.ts",
  "browser": "build/browser/axiosrx.bundle.min.js",
  "files": [
    "build",
    "src"
  ],
  "scripts": {
    "prepublishOnly": "yarn run build && yarn run test",
    "clean": "rimraf build __test__ coverage .nyc_output",
    "build": "concurrently -n \"es5,es6,esm/browser\" -c \"green,cyan,blue\" \"yarn run build-es5\" \"yarn run build-es6\" \"yarn run build-esm+bundles\"",
    "build-es5": "rimraf build/es5 && tsc -p src/tsconfig.es5.json",
    "build-es6": "rimraf build/es6 && tsc -p src/tsconfig.es6.json",
    "build-esm": "rimraf build/esm && tsc -p src/tsconfig.esm.json",
    "build-esm+bundles": "rimraf build/webpack && yarn run build-esm && webpack --config webpack.config.js",
    "build-test": "rimraf __test__ && tsc -p test/tsconfig.test.json",
    "test": "yarn run build-test && mocha --opts mocha.opts",
    "test+coverage": "rimraf coverage && nyc --nycrc-path nyc.json yarn run test",
    "tslint": "tslint -p src/tsconfig.es5.json -c src/tslint.json --fix"
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/mocha": "^2.2.48",
    "chai": "^4.1.2",
    "concurrently": "^3.5.1",
    "mocha": "^5.0.1",
    "mocha-typescript": "^1.1.12",
    "nyc": "~11.5.0",
    "rimraf": "^2.6.2",
    "rxjs": "^5.5.6",
    "source-map-loader": "^0.2.3",
    "source-map-support": "^0.5.3",
    "tslint": "^5.9.1",
    "typescript": "~2.6.2",
    "uglifyjs-webpack-plugin": "^1.2.0",
    "webpack": "^3.11.0"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "es6-promise": "^4.2.4",
    "tslib": "^1.9.0"
  },
  "peerDependencies": {
    "rxjs": "^5.5.6"
  }
}
