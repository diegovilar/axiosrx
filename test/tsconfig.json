{
  "$schema": "http://json.schemastore.org/tsconfig",
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "noEmit": false,
    "target": "es5",
    "module": "commonjs",
    "outDir": "../__test__",
    "downlevelIteration": true,
    "noEmitHelpers": false,
    "importHelpers": false,
    "strictPropertyInitialization": false, // only on TS 2.7+
    "inlineSources": false
  },
  "include": [
    "./",
    "../src",
    "../typings/**/*.d.ts"
  ]
}
