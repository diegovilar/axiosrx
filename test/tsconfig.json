{
  "$schema": "http://json.schemastore.org/tsconfig",
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "outDir": "../__test__",
    "downlevelIteration": true,
    "noEmitHelpers": false,
    "importHelpers": false,
    "strictPropertyInitialization": false, // only on TS 2.7+
    "inlineSources": false,
    "lib": [
      "es5",
      "es2015.promise",
      "dom"
    ]
  }
}
